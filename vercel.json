{
    "version": 2,
    "builds": [
      { "src": "backend/server.js", "use": "@vercel/node" },
      { "src": "public/**", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/submit", "dest": "backend/server.js" },
      { "src": "/users/all", "dest": "backend/server.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  